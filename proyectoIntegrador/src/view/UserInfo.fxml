<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400" prefWidth="600" styleClass="root" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserInfoController">
    <VBox alignment="CENTER" layoutX="100" layoutY="30" prefWidth="400" spacing="15" stylesheets="@Style.css">
        <Label styleClass="header-label" text="MI INFORMACION " />
        
        <HBox spacing="10">
            <Label fx:id="lblNombre" styleClass="info-label" />
        </HBox>

        <HBox spacing="10">
            <Label fx:id="lblCedula" styleClass="info-label" />
        </HBox>

        <HBox spacing="10">
            <Label fx:id="lblCorreo" styleClass="info-label" />
        </HBox>

        <HBox spacing="10">
            <Label fx:id="lblTelefono" styleClass="info-label" />
        </HBox>

        <HBox spacing="10">
            <Label fx:id="lblDepartamento" styleClass="info-label" />
        </HBox>

        <HBox spacing="10">
            <Label fx:id="lblTipoUsuario" styleClass="info-label" />
        </HBox>
    </VBox>
</AnchorPane>
